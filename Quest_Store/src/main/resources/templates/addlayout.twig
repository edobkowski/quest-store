        <link href="https://fonts.googleapis.com/css?family=Jua|Metal+Mania|Pacifico" rel="stylesheet">
    </head>
    <div id="contentbox" class="contentbox" style="flex-direction: column; align-items: center;">
        <body>
            <header class="navigationbar">
                <nav>
                    <a style="text-align: left" href="{{homePath}}">HOME</a>
                    <h1>Quest Store</h1>
                    <a href="">Account</a>
                </nav>
            </header>
            <div id="addbutton" class="addbutton animate">
                <button onclick="createClassForm()" id="addstudent">{{addButtonName}}</button>
            </div>
            <div id="studentsform">
                <form id="form_tag" name="form">
                <div id="form">
                </div>
                <div id="add"></div>
                </form>
                <div class="studentscontainer animate">

                    {% for element in elementsList %}
                        <div class="studentsbox">
                            <a href="">{{element}}</a>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </body>
    </div>
    <footer></footer>
    <script src="{{jsPath}}"></script>
</html>